{
  "name": "font-subset",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "emotion": "^9.2.12",
    "glob": "^7.1.3",
    "import-all.macro": "^2.0.3",
    "lodash": "^4.17.11",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.12",
    "react-scripts": "2.0.5",
    "rmfr": "^2.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "node --experimental-modules src/font-subset/subset.mjs && react-scripts build",
    "update": "yarn run doc && yarn run generateImportFont",
    "doc": "node --experimental-modules src/font-subset/doc/doc.mjs",
    "generateImportFont": "node --experimental-modules src/font-subset/generateImportFont.mjs",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
